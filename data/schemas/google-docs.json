{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Google Docs Tool Schema",
    "allOf": [
        {
            "$ref": "../tool-schema.json"
        },
        {
            "type": "object",
            "properties": {
                "settings": {
                    "type": "object",
                    "properties": {
                        "enable_markdown": {
                            "type": [
                                "boolean",
                                "null"
                            ],
                            "description": "Enable Markdown formatting support in Google Docs."
                        }
                    },
                    "additionalProperties": false
                }
            },
            "additionalProperties": false
        }
    ]
}