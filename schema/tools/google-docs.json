{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Google Docs Tool Schema",
    "allOf": [
        {
            "$ref": "../tool-schema.json"
        },
        {
            "type": "object",
            "required": [
                "default_settings"
            ],
            "properties": {
                "default_settings": {
                    "type": "object",
                    "required": [
                        "enable_markdown"
                    ],
                    "properties": {
                        "enable_markdown": {
                            "type": "boolean"
                        }
                    },
                    "additionalProperties": false
                }
            },
            "additionalProperties": false
        }
    ]
}